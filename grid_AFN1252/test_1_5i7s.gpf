npts 60 60 60                        # num.grid points in xyz
gridfld 5i7s.maps.fld                # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C N NA OA P SA      # receptor atom types
ligand_types A C HD N NA OA SA       # ligand atom types
receptor 5i7s.pdbqt                  # macromolecule
gridcenter 24.073 -10.919 -19.861    # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map edit_5i7s.A.map                  # atom-specific affinity map
map edit_5i7s.C.map                  # atom-specific affinity map
map edit_5i7s.HD.map                 # atom-specific affinity map
map edit_5i7s.N.map                  # atom-specific affinity map
map edit_5i7s.NA.map                 # atom-specific affinity map
map edit_5i7s.OA.map                 # atom-specific affinity map
map edit_5i7s.SA.map                 # atom-specific affinity map
elecmap 5i7s.e.map                   # electrostatic potential map
dsolvmap 5i7s.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
